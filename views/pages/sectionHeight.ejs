<body id="mainContentSection">
	<div id="mainContentSectionChild"></div>
</body>
<script>
	$(document).ready(function () {
		$.get("/content-section-height-data", function (result) {

			result.forEach(element => {
				// if(element.height>0){
				// }else{
				$.get("/content-section-get-entry/" + element.id, function (result) {
					$('#mainContentSectionChild').html(result);
					$('#mainContentSection').find('meta').remove();
					let height = $(document).height();
					$('#mainContentSection').append('<input id="heightValue" value="' + height + '"/>')
					//$('#mainContentSection').css({ height: height })
					let input = {
						id: element.id,
						pageid: element.pageid,
						height: height
					};
					$.post(`/section/update`, input, function (result) {
						console.log(result);
					});
				})
			// }
			});


		})

	}) 
</script>

</html>